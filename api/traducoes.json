---
layout: api
---
[
    {% for traducao in site.traducoes %}
      {
              "id"                : "{{ traducao.id }}",
              "title"             : {{ traducao.title  | escape | jsonify }},
              "system"            : {{ traducao.system  | escape | jsonify  }},
              "platform"          : {{ traducao.platform  | escape | jsonify }},
              "game_title"        : {{ traducao.game_title  | escape | jsonify }},
              "patch_author"      : {{ traducao.patch_author | escape | jsonify }},
              "patch_group"       : {{ traducao.patch_group  | escape | jsonify }},
              "url"               : {{ traducao.url | escape | jsonify }},
              "date"              : "{{ traducao.date }}",
              "content"           : {{ traducao.content | escape | strip_newlines | strip_html | jsonify }},
              "excerpt"           : {{ traducao.excerpt | escape | strip_newlines | strip_html | jsonify }}
      } {% if forloop.last %}{% else %},{% endif %}
    {% endfor %}
]