---
---
[
	{% for traducao in site.traducoes %}
		{
            "id"         : "{{ traducao.id }}",
            "title"      : "{{ traducao.title | escape }}",
            "category"   : "{{ traducao.category }}",
            "tags"       : "{{ traducao.tags | join: ', ' }}",
            "url"        : "{{ traducao.url }}",
            "path"       : "{{ traducao.path }}",
            "next"       : "{{ traducao.next.url }}",
            "previous"   : "{{ traducao.previous.url }}",
            "date"       : "{{ traducao.date }}",
            "shortdate"  : "{{ traducao.date | date : '%B %d, %Y' }}",
            "content"    : "{{ traducao.content | escape | strip_newlines }}",
            "excerpt"    : "{{ traducao.excerpt | escape | strip_newlines }}"
		} {% if forloop.last %}{% else %},{% endif %}
	{% endfor %}
]